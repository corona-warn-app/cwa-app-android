    <!-- ####################################
                Recycler Bin
    ###################################### -->

    <!-- XHED: recycler bin overview title -->
    <string name="recycler_bin_title">"Trash"</string>
    <!-- XHED: empty recycler bin label -->
    <string name="recycler_bin_empty_label">"The Trash is Empty"</string>
    <!-- XHED: empty recycler bin description -->
    <string name="recycler_bin_empty_description">"When you remove tests and certificates, they are moved to the trash and displayed here."</string>
    <!-- XHED: recycler bin remove all menu item -->
    <string name="recycler_bin_remove_all">"Remove All"</string>

    <!-- Recycle Bin  - Restore Dgc on scanning confirmation-->
    <!-- XHED: Recycle Bin  - Restore DGC dialog title -->
    <string name="recycle_bin_restore_dgc_dialog_title">"Restore Certificate"</string>
    <!-- XTXT: Recycle Bin  - Restore DGC dialog message -->
    <string name="recycle_bin_restore_dgc_dialog_message">"The QR code has already been registered on your smartphone. You can restore the certificate from teh trash. "</string>

    <!-- DGC  - Recycle certificate dialog-->
    <!-- XHED: Recycle Bin - recycle certificate dialog title -->
    <string name="recycle_bin_recycle_certificate_dialog_title">"Do you want to remove the certificate?"</string>
    <!-- XTXT: Recycle Bin - recycle certificate dialog message -->
    <string name="recycle_bin_recycle_certificate_dialog_message">"The certificate will be moved to the trash. You’ll find the trash on the “Status” tab page, under the three dots on the upper right. You can permanently delete or restore it there. The certificate will be deleted automatically after 30 days."</string>
    <!-- XBUT: Recycle Bin - recycle certificate dialog positive button -->
    <string name="recycle_bin_recycle_certificate_dialog_positive_button">"Remove Certificate"</string>
    <!-- XBUT: Recycle Bin - recycle certificate dialog negative button -->
    <string name="recycle_bin_recycle_certificate_dialog_negative_button">"Cancel"</string>

    <!-- XTXT: Recycle Bin - overview subheader -->
    <string name="recycle_bin_overview_subheader">"Tap the three dots next to an element to restore it. Elements will be deleted automatically after 30 days."</string>
    <!-- XTXT: Recycle Bin - restore -->
    <string name="recycle_bin_restore">"Restore"</string>
    <!-- XTXT: Recycle Bin - remove permanently -->
    <string name="recycle_bin_remove_permanently">"Delete Permanently"</string>

    <!-- Recycle bin remove all dialog-->
    <!-- XHED: Recycle Bin - remove all dialog title -->
    <string name="recycle_bin_remove_all_dialog_title">"Do you want to empty the trash?"</string>
    <!-- XTXT: Recycle Bin - remove all dialog message -->
    <string name="recycle_bin_remove_all_dialog_message">"You will no longer be able to restore its contents."</string>
    <!-- XBUT: Recycle Bin - remove all dialog positive button -->
    <string name="recycle_bin_remove_all_dialog_positive_button">"Remove"</string>
    <!-- XBUT: Recycle Bin - remove all dialog negative button -->
    <string name="recycle_bin_remove_all_dialog_negative_button">"Cancel"</string>

    <!-- Recycle bin restore certificate dialog-->
    <!-- XHED: Recycle Bin - restore certificate dialog title -->
    <string name="recycle_bin_restore_certificate_dialog_title">"Restore Certificate"</string>
    <!-- XTXT: Recycle Bin - restore certificate dialog message -->
    <string name="recycle_bin_restore_certificate_dialog_message">"The restored certificate will be moved to its original location. You will then find it on the “Certificates” tab page."</string>
    <!-- XBUT: Recycle Bin - restore certificate dialog positive button -->
    <string name="recycle_bin_restore_certificate_dialog_positive_button">"Restore"</string>
    <!-- XBUT: Recycle Bin - restore certificate dialog negative button -->
    <string name="recycle_bin_restore_certificate_dialog_negative_button">"Cancel"</string>

    <!-- Recycle bin restore test dialog-->
    <!-- XHED: Recycle Bin - restore test dialog title -->
    <string name="recycle_bin_restore_test_dialog_title">"Restore Test"</string>
    <!-- XTXT: Recycle Bin - restore test dialog message -->
    <string name="recycle_bin_restore_test_dialog_message">"The restored test will be moved to its original location. You will then find it on the “Status” tab page."</string>
    <!-- XBUT: Recycle Bin - restore test dialog positive button -->
    <string name="recycle_bin_restore_test_dialog_positive_button">"Restore"</string>
    <!-- XBUT: Recycle Bin - restore test dialog negative button -->
    <string name="recycle_bin_restore_test_dialog_negative_button">"Cancel"</string>
