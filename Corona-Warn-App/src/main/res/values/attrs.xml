<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">
        <attr name="progressColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="progress" format="float" />
        <attr name="circleWidth" format="float" />
        <attr name="disableText" format="boolean" />
    </declare-styleable>
    <declare-styleable name="StepEntry">
        <attr name="step_entry_icon" format="integer" />
        <attr name="step_entry_final" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SimpleStepEntry">
        <attr name="simple_step_entry_title" format="string" />
        <attr name="simple_step_entry_text" format="string" />
    </declare-styleable>
    <declare-styleable name="BulletPointList">
        <attr name="entries" format="reference" />
    </declare-styleable>
    <declare-styleable name="TestResultSection">
        <attr name="test_result_section_headline" format="string" />
        <attr name="test_result_section_content" format="string" />
        <attr name="test_result_section_status_icon" format="reference" />
        <attr name="test_result_section_registered_at_text" format="string" />
    </declare-styleable>

    <declare-styleable name="TracingCardInfoRow">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:icon" />
        <attr name="compatIconTint" format="reference" />
    </declare-styleable>
    <declare-styleable name="TracingDetailsBehaviorRow">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:icon" />
        <attr name="android:backgroundTint" />
        <attr name="android:foregroundTint" />
    </declare-styleable>
    <declare-styleable name="QrViewfinderView">
        <attr name="maskSize" format="dimension" />
        <attr name="maskPosition" format="float" />
        <attr name="maskColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MoreInformationView">
        <attr name="isTopDividerVisible" format="boolean" />
        <attr name="isBottomDividerVisible" format="boolean" />
        <attr name="isToggleVisible" format="boolean" />
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="subtitleText" format="string" />
        <attr name="toggleOnText" format="string" />
        <attr name="toggleOffText" format="string" />
        <attr name="android:checked" />
    </declare-styleable>

    <declare-styleable name="DispatcherCardView">
        <attr name="headline" format="string" />
        <attr name="body" format="string" />
        <attr name="illustration" format="integer" />
        <attr name="topIcon" format="integer" />
        <attr name="cardBackground" format="integer" />
        <attr name="colorForText" format="color" />
        <attr name="iconTint" format="color" />
    </declare-styleable>

    <item name="default_button" type="id" />
    <item name="checkins_fragment_menu_id" type="id" />
    <item name="contact_diary_onboarding_fragment_menu_id" type="id" />
    <item name="contact_diary_overview_fragment_menu_id" type="id" />
    <item name="trace_locations_fragment_menu_id" type="id" />
    <item name="contact_diary_comment_info_fragment_navigation_icon_buttonId" type="id" />
    <item name="contact_diary_edit_persons_fragment_navigation_icon_buttonId" type="id" />
    <item name="home_fragment_menu_id" type="id" />
    <item name="certificate_detail_fragment_menu_id" type="id" />
    <item name="test_certificate_detail_fragment_navigation_icon_buttonId" type="id" />
    <item name="trace_location_organizer_warn_info_fragment_title_id" type="id" />
    <item name="trace_location_organizer_warn_duration_fragment_title_id" type="id" />
    <item name="trace_location_organizer_warn_tan_fragment_title_id" type="id" />
    <item name="trace_location_organizer_trace_locations_warn_list_fragment_title_id" type="id" />
    <item name="trace_location_organizer_category_fragment_title_id" type="id" />
    <item name="trace_location_create_fragment_title_id" type="id" />
    <item name="trace_location_create_fragment_subtitle_id" type="id" />
    <item name="trace_location_organizer_trace_locations_list_fragment_title_id" type="id" />

    <declare-styleable name="ProgressLoadingButton">
        <attr name="buttonText" format="string" />
        <attr name="loadingText" format="string" />
        <attr name="isLoading" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TraceLocationHighlightView">
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="DccReissuanceBulletPoint">
        <attr name="android:text" />
        <attr name="bold" format="boolean" />
    </declare-styleable>
</resources>
