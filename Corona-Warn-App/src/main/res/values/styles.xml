    <style name="materialSubtitleSixteen" parent="@style/TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/colorOnPrimary</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="subtitleBoldSixteen" parent="materialSubtitleSixteen">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="subtitleSemanticRed" parent="@style/subtitle">
        <item name="android:textColor">@color/colorTextSemanticRed</item>
    </style>

    <style name="subtitleMedium" parent="@style/TextAppearance.Material3.BodyLarge">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorTextPrimary2</item>
        <item name="android:textStyle">normal</item>
        <item name="fontWeight">400</item>
    </style>

    <style name="subtitleMediumBold" parent="@style/subtitleMedium">
        <item name="android:textStyle">bold</item>
        <item name="fontWeight">600</item>
    </style>

    <style name="body1" parent="@style/TextAppearance.Material3.BodyLarge">
        <item name="android:textColor">@color/colorOnPrimary</item>
    </style>

    <style name="body1Medium" parent="@style/TextAppearance.Material3.BodyLarge">
        <item name="android:textColor">@color/colorTextPrimary2</item>
    </style>

    <style name="body2" parent="@style/TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/colorOnPrimary</item>
    </style>

    <style name="menu_text" parent="@style/TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/menu_text_color</item>
    </style>

    <style name="body2Medium" parent="@style/TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/colorTextPrimary2</item>
    </style>

    <style name="bodyButton" parent="@style/TextAppearance.Material3.LabelLarge">
