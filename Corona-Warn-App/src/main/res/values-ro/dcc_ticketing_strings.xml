    <!-- DCC Ticketing: Consent One -->
    <!-- XHED: DCC Ticketing: Consent One: Header -->
    <string name="dcc_ticketing_consent_one_header">"Consimțământul dvs."</string>
    <!-- XACT: DCC Ticketing: Consent One: Accessibility -->
    <string name="dcc_ticketing_consent_one_acessibility">"Un smartphone este utilizat pentru a scana un cod QR dintr-un sistem de bilete. Este stabilită o conexiune între sistemul de bilele și smartphone."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Title -->
    <string name="dcc_ticketing_consent_one_title">"Validarea certificatului la emiterea biletului"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Provider -->
    <string name="dcc_ticketing_consent_one_provider">"Furnizor:"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Provider Default Value -->
    <string name="dcc_ticketing_consent_one_provider_default">"„Lufthansa”"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Booking -->
    <string name="dcc_ticketing_consent_one_booking">"Rezervare:"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Booking  Deafault Value-->
    <string name="dcc_ticketing_consent_one_booking_default">"„1234”:"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Body -->
    <string name="dcc_ticketing_consent_one_body">"Furnizorul dorește să se asigure că este disponibil un certificat valabil pentru rezervarea dvs. Consimțământul dvs. este necesar pentru a obține detaliile de emitere a biletului de la furnizor și pentru elibera certificatele dvs. salvate."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Point 1 -->
    <string name="dcc_ticketing_consent_one_point_1">"Nu aveți obligația să permiteți validarea certificatului dvs. cu aplicația Corona-Warn. De asemenea, puteți prezenta dovada vaccinării dvs., a testării sau starea recuperării după boală într-un mod diferit."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Point 2 -->
    <string name="dcc_ticketing_consent_one_point_2">"Cerințele certificatului sunt definite de către furnizor. RKI nu are nicio influență asupra acestui proces. Contactați furnizorul pentru a obține mai multe informații despre cerințele certificatului."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Point 3 -->
    <string name="dcc_ticketing_consent_one_point_3">"Identificarea certificatelor corespunzătoare se efectuează local pe smartphone-ul dvs. Nu sunt expediate niciun fel de date la RKI sau la furnizor."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Point 4 -->
    <string name="dcc_ticketing_consent_one_point_4">"RKI nu primește date privind emiterea biletelor dvs. sau privind certificatele pe care le-ați salvat."</string>
    <!-- XTXT: DCC Ticketing: Consent One: Data protection -->
    <string name="dcc_ticketing_consent_one_data_protection">"Pentru mai multe informații despre prelucrarea datelor, consultați înștiințarea de confidențialitate."</string>
    <!-- XBUT: DCC Ticketing: Consent One: Consent Button -->
    <string name="dcc_ticketing_consent_one_consent_btn">"Accept"</string>
    <!-- XBUT: DCC Ticketing: Consent One: Cancel Button -->
    <string name="dcc_ticketing_consent_one_cancel_btn">"Anulare"</string>

    <!-- XHED: DCC Ticketing: Consent One: Cancel Dialog: Title  -->
    <string name="dcc_ticketing_consent_one_cancel_dialog_title">"Doriți să anulați validarea?"</string>
    <!-- XTXT: DCC Ticketing: Consent One: Cancel Dialog: Body  -->
    <string name="dcc_ticketing_consent_one_cancel_dialog_body">"Dacă anulați validarea, certificatul nu poate fi validat."</string>
    <!-- XBUT: DCC Ticketing: Consent One: Cancel Dialog: Continue Button  -->
    <string name="dcc_ticketing_consent_one_cancel_dialog_continue_btn">"Continuare validare"</string>
    <!-- XBUT: DCC Ticketing: Consent One: Cancel Dialog: Cancel Button  -->
    <string name="dcc_ticketing_consent_one_cancel_dialog_cancel_btn">"Anulare validare"</string>

    <!-- Not in Dcc ticketing allow list dialog-->
    <!-- XTXT: not in AllowList dialog title -->
    <string name="dcc_ticketing_not_in_allow_list_dialog_title">"Notă de securitate"</string>
    <!-- XTXT: not in AllowList dialog message -->
    <string name="dcc_ticketing_not_in_allow_list_dialog_message">"Acest furnizor este necunoscut pentru noi. Din motive de securitate, validarea la emiterea biletului este posibilă doar pentru furnizorii cunoscuți. Contactați furnizorul care v-a transmis acest cod QR."</string>

    <!-- DCC Ticketing: Certificate Selection Screen -->
    <!-- XHED: Certificate Selection Screen Toolbar Title -->
    <string name="dcc_ticketing_certificate_selection_title">"Selectare certificat"</string>
    <!-- XTXT: Certificate Selection Screen: No Certificates Found Title -->
    <string name="dcc_ticketing_certificate_selection_no_certificates_found_title">"Niciun certificat corespunzător nu este disponibil."</string>
    <!-- XTXT: Certificate Selection Screen: No Certificates Found Subtitle -->
    <string name="dcc_ticketing_certificate_selection_no_certificates_found_subtitle">"Nu aveți un certificat care îndeplinește cerințele sistemului de bilete. Asigurați-vă că numele dvs. din sistemul de bilete corespunde cu numele de pe certificatele dvs. Dacă aveți nevoie de certificate suplimentare care nu sunt încă disponibile în aplicație, adăugați-le acum și apoi începeți din nou validarea."</string>
    <!-- XTXT: Certificate Selection Screen: No Certificates Found: Requested Certificates Text -->
    <string name="dcc_ticketing_certificate_selection_requested_certificates_text">"Următorul certificat este necesar:"</string>
    <!-- XTXT: Certificate Selection Screen: More Information -->
