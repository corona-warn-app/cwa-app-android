    <!-- Error Handling -->
    <!-- XTXT: Service Provider (without provider name) error message-->
    <string name="dcc_ticketing_error_service_provider_error_no_name">"A apărut o eroare la conectarea la furnizor. Contactați furnizorul dacă problema persistă."</string>
    <!-- XTXT: Service Provider error message-->
    <string name="dcc_ticketing_error_service_provider_error">"A apărut o eroare la conectarea la furnizorul %1$s. Contactați furnizorul dacă problema persistă."</string>
