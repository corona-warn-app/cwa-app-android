    <!-- XBUT: Recycle Bin - recycle certificate dialog positive button -->
    <string name="recycle_bin_recycle_certificate_dialog_positive_button">"Sertifikayı Sil"</string>
    <!-- XBUT: Recycle Bin - recycle certificate dialog negative button -->
    <string name="recycle_bin_recycle_certificate_dialog_negative_button">"İptal"</string>

